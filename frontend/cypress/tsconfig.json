{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["cypress", "cypress-plugin-stripe-elements"],
    "baseUrl": "./"
  },
  "include": ["**/*.ts"]
}
